{
  "name": "react-vite-boilerplate",
  "version": "1.0.0",
  "description": "ReactJS & Vite boilerplate",
  "main": "main.tsx",
  "type": "module",
  "scripts": {
    "start": "vite --open",
    "dev": "vite --host",
    "test": "vitest run",
    "test:interactive": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext ts,tsx",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "preview": "vite preview",
    "tsc": "tsc",
    "build": "tsc && vite build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Grimowsky/react-vite-boilerplate.git"
  },
  "keywords": [
    "react",
    "vite",
    "boilerplate"
  ],
  "author": "Grimm",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Grimowsky/react-vite-boilerplate/issues"
  },
  "homepage": "https://github.com/Grimowsky/react-vite-boilerplate#readme",
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.7.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "history": "^5.3.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "typescript": "5.9.2",
    "vite": "^7.1.0",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.2.4"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-grimowsky"
    ],
    "root": true
  }
}
